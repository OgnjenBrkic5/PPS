        '                                                                ' +
        '  MATCH [FULL]'
      '                                    NOT NULL,'
      '    TekstualnaPoruka VARCHAR (250),'
      '    Ocena            INTEGER (1, 5) NOT NULL,'
      '    PRIMARY KEY ('
      '        IDKorisnika,'
      '        IDRadnika'
      '    ),'
      '    FOREIGN KEY ('
      '        IDKorisnika'
      '    )'
      '    REFERENCES Korisnik (IDKorisnika) ON UPDATE CASCADE,'
